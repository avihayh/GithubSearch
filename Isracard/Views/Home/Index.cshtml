@{
    ViewBag.Title = "Home Page";
}
<script src="~/Scripts/GithabSearch.js"></script>
<div class="jumbotron">
    <h1>Githab Search API</h1>
    <p class="lead">In this site you can search Githab projects easily</p>
    <p><a href="#" class="btn btn-primary btn-lg">Go to Githab</a></p>
</div>

<form action="~/Search/Search" method="GET">
    <div class="container">
        <div class="col-md-3">
            <form class="navbar-form" role="search">
                <div class="input-group add-on">
                    <input class="form-control" placeholder="Search" name="srch-term" id="srch-term" type="text">
                    <div class="input-group-btn">
                        <button class="btn btn-default" type="submit"><i class="glyphicon glyphicon-search"></i></button>
                    </div>
                </div>
            </form>
        </div>
    </div>
</form>
    @*@Html.Partial("SearchResults");*@

